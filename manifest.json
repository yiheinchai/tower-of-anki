{
  "manifest_version": 3,
  "name": "AnkiWeb Supercharged",
  "version": "0.0.4",
  "description": "AnkiWeb support for dark mode and keyboard shortcuts",
  "icons": {
    "16": "images/anki16.png",
    "32": "images/anki32.png",
    "48": "images/anki48.png",
    "128": "images/anki128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://ankiuser.net/*"],
      "js": ["scripts/content.js"]
    }
  ]
}
